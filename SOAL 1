def perkalian(a, b):
    hasil = 0
    for i in range(b):
        hasil += a
    return hasil

angka1 = 6
angka2 = 5
hasil_perkalian = perkalian(angka1, angka2)
print(f"{angka1} x {angka2} = {' + '.join(str(angka1) for _ in range(angka2))} = {hasil_perkalian}")

angka1 = 7
angka2 = 10
hasil_perkalian = perkalian(angka1, angka2)
print(f"{angka1} x {angka2} = {' + '.join(str(angka1) for _ in range(angka2))} = {hasil_perkalian}")
